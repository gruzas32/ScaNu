<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="155.0" prefWidth="183.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.demo.LoginController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Username" />
      <TextField fx:id="login" layoutX="14.0" layoutY="31.0" />
      <Label layoutX="14.0" layoutY="56.0" text="Password" />
      <PasswordField fx:id="password" layoutX="14.0" layoutY="75.0" />
      <Button layoutX="18.0" layoutY="109.0" mnemonicParsing="false" onAction="#UserLogin" text="Login" />
      <Button layoutX="104.0" layoutY="109.0" mnemonicParsing="false" onAction="#UserRegister" text="Register" />
   </children>
</AnchorPane>
